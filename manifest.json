{
  "manifest_version": 3,
  "name": "Promlens Datasource Manager",
  "version": "1.0.0",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "storage",
    "contextMenus"
  ]
}